{
  "name": "Netbanking",
  "private": true,
  "scripts": {
    "build": "npm run prisma-generate && turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "dev:docker": "npm run dev:prisma && npm run dev",
    "dev:prisma": "cd packages/db && npx prisma migrate dev && npx prisma db seed",
    "prisma-generate": "cd packages/db && npx prisma generate",
    "docker-compose": "sudo docker-compose --env-file ./packages/db/.env up"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "esbuild": "0.21.1",
    "prettier": "^3.2.5",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.5.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "axios": "^1.6.8",
    "dotenv": "^16.4.5",
    "react-router-dom": "^6.23.1"
  }
}